<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" prefHeight="447.0" prefWidth="323.0" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.okcu.guessfx.GuessFXController">
    <padding>
        <Insets bottom="50.0" left="50.0" right="50.0" top="50.0" />
    </padding>
   <Label text="Guess a number between 1 - 100" />
   <TextField fx:id="txtUserGuess" prefHeight="26.0" prefWidth="300.0" />
   <Label text="High or Low:" />

    <Label fx:id="welcomeText" />
   <Label fx:id="txtNumGuesses" text="Number of Guesses:" />

    <Button fx:id="btnGuess" onAction="#onGuess" text="Guess" />
</VBox>
