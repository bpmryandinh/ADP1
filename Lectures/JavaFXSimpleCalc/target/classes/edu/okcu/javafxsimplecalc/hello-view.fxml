<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.okcu.javafxsimplecalc.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <children>
      <Label text="Please Enter a Number" />
      <TextField fx:id="inputTextField" />
      <Button mnemonicParsing="false" text="Square" onAction="#onSquareButtonClick" />
      <Button mnemonicParsing="false" text="Cube" onAction="#onCubeButtonClick" />
      <Label text="Result" />
      <Label text="0.0" fx:id="resultsLabel" />
   </children>


</VBox>
